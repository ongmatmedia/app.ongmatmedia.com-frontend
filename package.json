    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "graphql": "14.6.0",
    "html-webpack-plugin": "^4.0.3",
    "husky": "^4.2.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.8",
    "lodash-webpack-plugin": "^0.11.5",
